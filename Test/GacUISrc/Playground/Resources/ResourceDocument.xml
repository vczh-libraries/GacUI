<Resource>
  <Folder name="EmbeddedDocument">
    <Image content="File" name="Gaclib">Gaclib.png</Image>
    <Doc content="File" name="Document">Document.xml</Doc>
  </Folder>
  <Instance name="MainWindowResource">
    <Instance ref.CodeBehind="false" ref.Class="demo::TestWindow" xmlns:demo="demo::*">
      <Window ref.Name="self" Text="Document" ClientSize="x:800 y:600">
        <att.BoundsComposition-set PreferredMinSize="x:800 y:600"/>
        <Table CellPadding="5" MinSizeLimitation="LimitToElementAndChildren" AlignmentToParent="left:0 top:0 right:0 bottom:0">
          <att.Rows>
            <_>composeType:MinSize</_>
            <_>composeType:MinSize</_>
            <_>composeType:MinSize</_>
            <_>composeType:Percentage percentage:1.0</_>
          </att.Rows>
          <att.Columns>
            <_>composeType:Percentage percentage:1.0</_>
            <_>composeType:Percentage percentage:1.0</_>
          </att.Columns>

          <Cell Site="row:0 column:0">
            <Label Text="GetDocumentLabelDefaultConfig()"/>
          </Cell>
          <Cell Site="row:0 column:1">
            <Table MinSizeLimitation="LimitToElementAndChildren" AlignmentToParent="left:0 top:0 right:0 bottom:0">
              <att.Rows>
                <_>composeType:MinSize</_>
              </att.Rows>
              <att.Columns>
                <_>composeType:MinSize</_>
                <_>composeType:Percentage percentage:1.0</_>
                <_>composeType:MinSize</_>
              </att.Columns>
              <Cell Site="row:0 column:0">
                <Label Text="GetSinglelineTextBoxDefaultConfig()"/>
              </Cell>
              <Cell Site="row:0 column:2">
                <CheckBox ref.Name="checkBoxPasswordChar" Text="PasswordChar(*)"/>
              </Cell>
            </Table>
          </Cell>
          <Cell Site="row:2 column:0">
            <Label Text="GetDocumentViewerDefaultConfig()"/>
          </Cell>
          <Cell Site="row:2 column:1">
            <Label Text="GetMultilineTextBoxDefaultConfig()"/>
          </Cell>

          <Cell Site="row:1 column:0">
            <DocumentTextBox EditMode="Editable" Document-uri="res://EmbeddedDocument/Document">
              <att.BoundsComposition-set AlignmentToParent="left:0 top:0 right:0 bottom:-1"/>
            </DocumentTextBox>
          </Cell>
          <Cell Site="row:3 column:0">
            <DocumentViewer EditMode="Editable" Document-uri="res://EmbeddedDocument/Document">
              <att.BoundsComposition-set AlignmentToParent="left:0 top:0 right:0 bottom:0"/>
            </DocumentViewer>
          </Cell>

          <Cell Site="row:1 column:1">
            <SinglelineTextBox Document-uri="res://EmbeddedDocument/Document">
              <att.BoundsComposition-set AlignmentToParent="left:0 top:0 right:0 bottom:-1"/>
              <att.PasswordChar-bind>cast char (checkBoxPasswordChar.Selected ? '*' : '')</att.PasswordChar-bind>
            </SinglelineTextBox>
          </Cell>
          <Cell Site="row:3 column:1">
            <MultilineTextBox Document-uri="res://EmbeddedDocument/Document">
              <att.BoundsComposition-set AlignmentToParent="left:0 top:0 right:0 bottom:0"/>
            </MultilineTextBox>
          </Cell>
        </Table>
      </Window>
    </Instance>
  </Instance>
</Resource>